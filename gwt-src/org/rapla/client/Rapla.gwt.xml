<?xml version="1.0" encoding="UTF-8"?>
<!--
  When updating your version of GWT, you should also update this DTD reference,
  so that your app can take advantage of the latest GWT module capabilities.
-->
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.5.1//EN"
  "http://google-web-toolkit.googlecode.com/svn/tags/2.5.1/distro-source/core/src/gwt-module.dtd">
<module rename-to='Rapla'>
  <!-- Inherit the core Web Toolkit stuff.                        -->
  <inherits name="com.google.gwt.xml.XML" />
  <inherits name='com.google.gwt.user.User'/>
  <inherits name="com.google.gwt.http.HTTP"/>
  <inherits name='com.google.gwt.logging.Logging'/>
  
  <inherits name="com.blogspot.ctasada.gwt.eureka.Eureka"/>
    <inherits name="com.blogspot.ctasada.gwt.eureka.theme.standard.Standard"/>
  
  <!-- Inherit the default GWT style sheet.  You can change       -->
  <!-- the theme of your GWT application by uncommenting          -->
  <!-- any one of the following lines.                            -->
  <inherits name='com.google.gwt.user.theme.clean.Clean'/>
  <inherits name="com.google.gwt.inject.Inject"/>
  <inherits name='org.rapla.Rapla_main_module'/>
  <!-- <inherits name='com.google.gwt.user.theme.standard.Standard'/> -->
  <!-- <inherits name='com.google.gwt.user.theme.chrome.Chrome'/> -->
  <!-- <inherits name='com.google.gwt.user.theme.dark.Dark'/>     -->
  <source path=''/>
  <!-- Other module inherits                                      -->

  <!-- Specify the app entry point class.                         -->
  <entry-point class='org.rapla.client.gwt.Rapla'/>
 
  <set-property name="user.agent" value="safari" />
  
  
  <!-- English language, independent of country -->
    <inherits name="com.google.gwt.i18n.CldrLocales" />
    <inherits name="com.google.gwt.i18n.I18N" />

    <set-configuration-property name="locale.searchorder"
        value="cookie,queryparam" />
    <set-configuration-property name="locale.useragent"
        value="Y" />

    <extend-property name="locale" values="de" />
    <set-property name="locale" value="de" />
    <set-property-fallback name="locale" value="de" />
  <!-- directives -->
  
  <set-property name="gwt.logging.logLevel" value="INFO"/>       
  <set-property name="gwt.logging.enabled" value="TRUE"/>  
  
  <!-- SuperDevMode -->
  <set-configuration-property name="devModeRedirectEnabled" value="true"/>
  
  <define-configuration-property name="extra.ginModules" is-multi-valued="true" />
  <!--  Replace with your ReservationGWTModule -->
  <extend-configuration-property name="extra.ginModules" value="org.rapla.client.edit.reservation.impl.ReservationControllerPluginGinModule" />

  
  
</module>
